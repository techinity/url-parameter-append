{
  "coveragePathIgnorePatterns": [
    "jest.setup.js"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "globals": {
    "context": {}
  },
  "testPathIgnorePatterns": [
    "/node_modules/"
  ],
  "testMatch": [
    "**/?(*.)spec.js"
  ],
  "verbose": true
}
